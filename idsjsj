  .Stack--dir-inline-whenWide > .Stack-divider, .Stack--dir-inline-whenWide > .Stack-item > .Stack-divider { border-inline-end: var(--borderWidth-thin, var(--borderWidth-thin)) solid var(--Stack-divider-color); inline-size: 0px; block-size: auto; }
  .Stack-item--expand-whenWide { flex-grow: 1; }
  .Stack-item--keepSize-whenWide { flex-shrink: 0; }
